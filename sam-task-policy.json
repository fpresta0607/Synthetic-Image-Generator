{
  "Version": "2012-10-17",
  "Statement": [
    { "Effect": "Allow", "Action": ["s3:GetObject","s3:ListBucket","s3:PutObject"], "Resource": [
      "arn:aws:s3:::sam-datasets-prod-401753844565",
      "arn:aws:s3:::sam-datasets-prod-401753844565/*",
      "arn:aws:s3:::sam-outputs-prod-401753844565",
      "arn:aws:s3:::sam-outputs-prod-401753844565/*",
      "arn:aws:s3:::sam-models-prod-401753844565",
      "arn:aws:s3:::sam-models-prod-401753844565/*"
    ]},
    { "Effect": "Allow", "Action": ["sqs:ReceiveMessage","sqs:DeleteMessage","sqs:GetQueueAttributes","sqs:SendMessage"], "Resource": "arn:aws:sqs:us-east-1:401753844565:sam-jobs" },
    { "Effect": "Allow", "Action": ["dynamodb:PutItem","dynamodb:UpdateItem","dynamodb:GetItem","dynamodb:DescribeTable"], "Resource": "arn:aws:dynamodb:us-east-1:401753844565:table/sam_jobs" },
    { "Effect": "Allow", "Action": ["logs:CreateLogStream","logs:PutLogEvents"], "Resource": "*" }
  ]
}
