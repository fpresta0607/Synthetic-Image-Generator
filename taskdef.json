{
    "memory":  "2048",
    "executionRoleArn":  "arn:aws:iam::\u003cacct\u003e:role/ecsTaskExecutionRole",
    "family":  "photosynth-full",
    "networkMode":  "awsvpc",
    "requiresCompatibilities":  [
                                    "FARGATE"
                                ],
    "containerDefinitions":  [
                                 {
                                     "name":  "photosynth-full",
                                     "logConfiguration":  {
                                                              "logDriver":  "awslogs",
                                                              "options":  {
                                                                              "awslogs-region":  "us-east-1",
                                                                              "awslogs-stream-prefix":  "ecs",
                                                                              "awslogs-group":  "/ecs/photosynth-full"
                                                                          }
                                                          },
                                     "essential":  true,
                                     "image":  "PLACEHOLDER",
                                     "portMappings":  [
                                                          {
                                                              "protocol":  "tcp",
                                                              "containerPort":  3000
                                                          },
                                                          {
                                                              "protocol":  "tcp",
                                                              "containerPort":  5001
                                                          }
                                                      ],
                                     "healthCheck":  {
                                                         "retries":  3,
                                                         "command":  [
                                                                         "CMD-SHELL",
                                                                         "curl -f http://localhost:3000/api/backend/health || exit 1"
                                                                     ],
                                                         "timeout":  5,
                                                         "interval":  30,
                                                         "startPeriod":  10
                                                     }
                                 }
                             ],
    "cpu":  "1024"
}
